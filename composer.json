{
    "name": "voiciano/amqp-topic-match",
    "type": "library",
    "description": "A utility for matching routing keys to AMQP-style topic patterns.",
    "keywords": ["amqp","amqp topic", "amqp match"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arutyunyan A.",
            "email": "voiciano@mail.ru"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "autoload": {
        "psr-4": {
            "Voiciano\\AMQPTopicMatch\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^v5.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Voiciano\\AMQPTopicMatch\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
